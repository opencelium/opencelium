<?xml version="1.0" encoding="UTF-8" standalone="no"?><invoker type="RESTful">
    <name>Aruba Airwave</name>
    <description/>
    <hint>This interface provides a token auth.</hint>
    <requiredData>
        <item name="url" type="string" visibility="public"/>
        <item name="username" type="string" visibility="public"/>
        <item name="password" type="string" visibility="protected"/>
        <item name="token" type="string" visibility="private">%{Login.header.X-BISCOTTI}</item>
    </requiredData>
    <authType>token</authType>
    <operations>
        <operation name="Login" type="test">
            <request>
                <method>POST</method>
                <endpoint>{url}/LOGIN</endpoint>
                <body>
                    <field name="destination" type="string">/</field>  
                    <field name="credential_0" type="string">{username}</field>  
                    <field name="credential_1" type="string">{password}</field>                    
                </body>
                <header>
                    <item name="Content-Type" type="string">application/x-www-form-urlencoded</item>
                </header>
            </request>
            <response>
                <success status="200">
                    <header>
                        <item name="X-BISCOTTI" type="string"/>
                    	<item name="Content-Type" type="string">application/x-www-form-urlencoded</item>
                    </header>
                    <body data="raw" format="string" type="object">
                    </body>
                </success>
                <fail status="401">
                    <body data="raw" format="string" type="object">
                    </body>
                </fail>
            </response>
        </operation>
