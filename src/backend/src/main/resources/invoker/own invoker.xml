<?xml version="1.0" encoding="UTF-8" standalone="no"?><invoker type="RESTful">
    <name>own invoker</name>
    <description>description</description>
    <hint>hint</hint>
    <icon/>
    <requiredData>
        <item name="url" type="string" visibility="public"/>
        <item name="apikey" type="string" visibility="public"/>
    </requiredData>
    <authType>apikey</authType>
    <operations>
        <operation name="test connection" type="test">
            <request>
                <method>GET</method>
                <endpoint>{url}/test/connection</endpoint>
                <body data="raw" format="json" type="object">
                </body>
                <header>
                    <item name="Content-Type" type="string">application/json</item>
                </header>
            </request>
            <response>
                <success status="200">
                    <body data="raw" format="json" type="object">
                        <field name="result" type="string"/>
                    </body>
                </success>
                <fail status="401">
                    <body data="raw" format="json" type="object">
                    </body>
                </fail>
            </response>
        </operation>
        <operation name="updateUser" type="">
            <request>
                <method>PUT</method>
                <endpoint>{url}/user</endpoint>
                <body data="raw" format="json" type="object">
                    <field name="id" type="string"/>
                    <field name="name" type="string"/>
                </body>
            </request>
            <response>
                <success status="200">
                    <body data="raw" format="json" type="object">
                    </body>
                </success>
                <fail status="404">
                    <body data="raw" format="json" type="object">
                    </body>
                </fail>
            </response>
        </operation>
        <operation name="addUser" type="">
            <request>
                <method>POST</method>
                <endpoint>{url}/user</endpoint>
                <body data="raw" format="json" type="object">
                    <field name="name" type="string"/>
                </body>
                <header>
                    <item name="Content-Type" type="string">application/json</item>
                </header>
            </request>
            <response>
                <success status="200">
                    <body data="raw" format="json" type="object">
                        <field name="result" type="string"/>
                    </body>
                </success>
                <fail status="404">
                    <body data="raw" format="json" type="object">
                    </body>
                </fail>
            </response>
        </operation>
        <operation name="getUsers" type="">
            <request>
                <method>GET</method>
                <endpoint>{url}/users</endpoint>
                <body data="raw" format="json" type="object">
                </body>
                <header>
                    <item name="Content-Type" type="string">application/json</item>
                </header>
            </request>
            <response>
                <success status="200">
                    <body data="raw" format="json" type="array">
                        <field name="id" type=""/>
                        <field name="name" type=""/>
                    </body>
                </success>
                <fail status="401">
                    <body data="raw" format="json" type="object">
                    </body>
                </fail>
            </response>
        </operation>
    </operations>
</invoker>