<invoker type="RESTful">
    <name>i-doit</name>
    <description>I-doit Description</description>
    <hint>hint text</hint>
    <icon>i-doit.png</icon>
    <requiredData>
        <item name="url" type="string" visibility="public"/>
        <item name="apikey" type="string" visibility="public"/>
    </requiredData>
    <authType>apikey</authType>
    <operations>
        <operation name="idoit.version" type="test">
            <request>
                <method>POST</method>
                <endpoint>{url}</endpoint>
                <header>
                    <item name="Content-Type" type="string">application/json</item>
                </header>
                <body>
                    <field name="version" type="string">2.0</field>
                    <field name="method" type="string">idoit.version</field>
                    <field name="params" type="object">
                        <field name="apikey" type="string">{apikey}</field>
                        <field name="language" type="string">de</field>
                    </field>
                    <field name="id" type="string">1</field>
                </body>
            </request>
            <response>
                <success status="200">
                    <header>
                         <item name="Content-Type" type="string"/>
                    </header>
                    <body>
                        <field name="result" type="object">
                            <field name="version" type="string" />
                        </field>
                    </body>
                </success>
                <fail status="200">
                    <header>
                        <item name="Content-Type" type="string" >application/json</item>
                    </header>
                    <body>
                        <field name="error" type="object">
                            <field name="code"  type="number"/>
                            <field name="message" type="string"/>
                            <field name="data" type="string"/>
                        </field>
                    </body>
                </fail>
            </response>
        </operation>
        <operation name="cmdb.objects.read" type="">
            <request>
                <method>POST</method>
                <endpoint>{url}</endpoint>
                <header>
                    <item name="Content-Type" type="string" >application/json</item>
                </header>
                <body>
                    <field name="version" type="string">2.0</field>
                    <field name="method" type="string">cmdb.objects.read</field>
                    <field name="params" type="object">
                        <field name="filter" type="object">
                            <field name="ids" type="array"/>
                            <field name="type" type="array"/>
                            <field name="title" type="string"/>
                            <field name="type_title" type="string"/>
                            <field name="sys_id" type="string"/>
                            <field name="firstname" type="string"/>
                            <field name="lastname" type="string"/>
                            <field name="email" type="string"/>
                        </field>
                        <field name="limit" type="string"/>
                        <field name="order_by" type="string"/>
                        <field name="sort" type="string"/>
                        <field name="apikey" type="string">{apikey}</field>
                        <field name="language" type="string"/>
                    </field>
                    <field name="id" type="string">1</field>
                </body>
            </request>
            <response>
                <success status="200">
                    <header>
                        <item name="Content-Type" type="string" >application/json</item>
                    </header>
                    <body>
                        <field name="id" type="number" required="true"/>
                        <field name="jsonrpc" type="string" required="true"/>
                        <field name="result" type="array">
                            <field name="id" type="string"/>
                            <field name="title" type="string"/>
                            <field name="sysid" type="string"/>
                            <field name="type" type="string"/>
                            <field name="created" type="string"/>
                            <field name="updated" type="string"/>
                            <field name="type_title" type="string"/>
                            <field name="type_group_title" type="string"/>
                            <field name="status" type="string"/>
                            <field name="cmdb_status" type="string"/>
                            <field name="cmdb_status_title" type="string"/>
                            <field name="image" type="string"/>
                        </field>
                    </body>
                </success>
                <fail status="200">
                    <header>
                        <item name="Content-Type" type="string" >application/json</item>
                    </header>
                    <body>
                        <field name="error" type="object">
                            <field name="code"  type="number"/>
                            <field name="message" type="string"/>
                            <field name="data" type="string"/>
                        </field>
                    </body>
                </fail>
            </response>
        </operation>
